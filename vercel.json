{
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/i18n/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://www.shirleymallea.com"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		}
	]
}
